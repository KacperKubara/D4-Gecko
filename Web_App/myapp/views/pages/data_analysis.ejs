<div class="jumbotron" style="background-image: url(./images/gecko_logo.png); 
 background-size: 100% 100%;">
    <h1>
        <%= title %>
    </h1>
    <p>Welcome to Gecko Web App</p>
</div>
<div class='jumbotron'>
    <h2 class='text-center'>Grip Pressure</h2>
    <hr>
    <div class='row'>
        <div class='col-md-6'>
            <canvas id="recent_grip" width="400" height="400"></canvas>
            <script src="https://cdnjs.cloudflare.com/ajax/libs/Chart.js/2.7.1/Chart.min.js"></script>
            <script src="./scripts/recent_grip.js"></script>
        </div>
        <div class='col-md-6'>
            <canvas id="all_grip" width="400" height="400"></canvas>
            <script src="https://cdnjs.cloudflare.com/ajax/libs/Chart.js/2.7.1/Chart.min.js"></script>
            <script src="./scripts/all_grip.js"></script>
        </div>
    </div>
</div>
